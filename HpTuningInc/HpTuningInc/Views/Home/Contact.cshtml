@{
    ViewBag.Title = "Contact";
}
<h2>@ViewBag.Title</h2>

<style>
    #map {
        height: 400px;
        width: 100%;
    }
</style>
<div id="map"></div>
<script>
    function initMap() {
        var uluru = { lat: 43.074703, lng: -88.0568769 };
        var map = new google.maps.Map(document.getElementById('map'), {
            zoom: 15,
            center: uluru
        });
        var marker = new google.maps.Marker({
            position: uluru,
            map: map
        });
    }
</script>
<script async defer
        src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBRmhFO-8UkxDnp6WnVmv9GoDKttOtUZSY&callback=initMap">
</script>



